<Window x:Class="VandaModaIntimaWpf.View.EntradaDeMercadoria.TelaRelatorioEntradaMercadoria"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VandaModaIntimaWpf.View.EntradaDeMercadoria"
        xmlns:RViewer="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        mc:Ignorable="d"
        Title="Entrada De Mercadoria"
        Height="450"
        Width="800"
        WindowState="Maximized">
    <Grid>

        <WindowsFormsHost x:Name="WindowsFormsHost">
            <RViewer:ReportViewer x:Name="EntradaMercadoriaReportViewer" Load="ReportV_Load" />
        </WindowsFormsHost>

    </Grid>
</Window>
