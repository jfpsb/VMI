<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="VandaModaIntimaWpf"
                   namespace="VandaModaIntimaWpf.Model">

	<class name="ComposicaoPreco" table="composicao_preco">
		<cache usage="read-write"/>
		<id name="Id" column="id" type="Int64">
			<generator class="hilo">
				<param name="max_lo">1024</param>
			</generator>
		</id>
		<many-to-one name="Loja" class="VandaModaIntimaWpf.Model.Loja" column="loja"/>
		<many-to-one name="ProdutoGrade" class="VandaModaIntimaWpf.Model.ProdutoGrade" column="produto_grade"/>
		<property name="Data" />
		<property name="PrecoCompra" />
		<property name="Frete" />
		<property name="CriadoEm" />
		<property name="ModificadoEm" />
		<property name="DeletadoEm" />
		<property name="Deletado" />
	</class>

</hibernate-mapping>
