<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="VandaModaIntimaWpf"
                   namespace="VandaModaIntimaWpf.Model">

  <class name="FolhaPagamento" table="folhapagamento" lazy="false">
    <composite-id>
      <key-property name="Mes" column="mes" />
      <key-property name="Ano" column="ano" />
      <key-many-to-one name="Funcionario" column="funcionario" class="VandaModaIntimaWpf.Model.Funcionario" />
    </composite-id>
    <property name="Valor" column="valor" />
    <property name="Fechada" column="fechada" />
  </class>

</hibernate-mapping>
