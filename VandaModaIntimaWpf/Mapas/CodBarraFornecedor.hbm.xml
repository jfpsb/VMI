<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="VandaModaIntimaWpf"
                   namespace="VandaModaIntimaWpf.Model.CodBarraFornecedor">

  <class name="CodBarraFornecedor" table="cod_barra_fornecedor" dynamic-insert="true" dynamic-update="true">
    <composite-id>
      <key-many-to-one name="Produto" column="produto" class="VandaModaIntimaWpf.Model.Produto.Produto" />
      <key-property name="Codigo" column="codigo" />
    </composite-id>
  </class>

</hibernate-mapping>
