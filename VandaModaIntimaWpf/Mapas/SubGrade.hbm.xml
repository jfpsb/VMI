<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="VandaModaIntimaWpf"
                   namespace="VandaModaIntimaWpf.Model">

  <class name="SubGrade" table="sub_grade" lazy="false">
    <composite-id>
      <key-many-to-one name="ProdutoGrade" class="VandaModaIntimaWpf.Model.ProdutoGrade" column="produto_grade"/>
      <key-many-to-one name="Grade" class="VandaModaIntimaWpf.Model.Grade" column="grade" />
    </composite-id>
  </class>

</hibernate-mapping>
