<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="VandaModaIntimaWpf"
                   namespace="VandaModaIntimaWpf.Model">

  <class name="RecebimentoCartao" table="recebimentocartao" dynamic-insert="true" dynamic-update="true">
    <composite-id>
      <key-property name="Mes" column="mes" />
      <key-property name="Ano" column="ano" />
      <key-many-to-one name="Loja" column="loja" class="VandaModaIntimaWpf.Model.Loja" />
      <key-many-to-one name="OperadoraCartao" column="operadoracartao" class="VandaModaIntimaWpf.Model.OperadoraCartao" />
    </composite-id>
    <property name="Valor" column="valor" />
  </class>

</hibernate-mapping>
