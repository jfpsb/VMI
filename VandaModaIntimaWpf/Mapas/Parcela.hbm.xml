<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="VandaModaIntimaWpf"
                   namespace="VandaModaIntimaWpf.Model">

  <class name="Parcela" table="parcela" lazy="false">
    <id name="Id" column="id" />
    <many-to-one name="Adiantamento" class="VandaModaIntimaWpf.Model.Adiantamento" lazy="false" />
    <many-to-one name="FolhaPagamento" class="VandaModaIntimaWpf.Model.FolhaPagamento" lazy="false" />
    <property name="Numero" column="numero" />
    <property name="Valor" column="valor" />
    <property name="Paga" column="paga" />
  </class>

</hibernate-mapping>
