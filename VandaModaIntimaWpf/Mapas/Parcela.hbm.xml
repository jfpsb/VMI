<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="VandaModaIntimaWpf"
                   namespace="VandaModaIntimaWpf.Model">

  <class name="Parcela" table="parcela" lazy="false">
    <composite-id>
      <key-property name="Mes" column="mes" />
      <key-property name="Ano" column="ano" />
      <key-many-to-one name="Adiantamento" column="adiantamento" class="VandaModaIntimaWpf.Model.Adiantamento" />
    </composite-id>
    <property name="Valor" column="valor" />
    <property name="Paga" column="paga" />
  </class>

</hibernate-mapping>
