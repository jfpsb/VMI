<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="VandaModaIntima"
                   namespace="VandaModaIntima.Model">

  <class name="Fornecedor" table="fornecedor" dynamic-insert="true" dynamic-update="true">
    <id name="Cnpj" column="cnpj"></id>
    <bag name="Produtos" table="produto" lazy="true" inverse="true">
      <key column="fornecedor" />
      <one-to-many class="Produto" />
    </bag>
    <property name="Nome" column="Nome" />
    <property name="NomeFantasia" column="fantasia" />
    <property name="Email" column="email" />
  </class>

</hibernate-mapping>
