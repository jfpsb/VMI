<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SincronizacaoVMI"
                   namespace="SincronizacaoVMI.Model">

	<class name="Marca" table="marca" dynamic-insert="true">
		<cache usage="read-write"/>
		<id name="Nome" />
		<property name="Uuid" />
		<many-to-one name="Fornecedor" class="SincronizacaoVMI.Model.Fornecedor" column="fornecedor" lazy="false" not-null="true"/>
		<property name="CriadoEm" />
		<property name="ModificadoEm" />
		<property name="DeletadoEm" />
		<property name="Deletado" />
	</class>

</hibernate-mapping>
