<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SincronizacaoVMI"
                   namespace="SincronizacaoVMI.Model.Pix">

	<class name="Calendario" table="pix_calendario">
		<id name="Id" column="id" type="Int32">
			<generator class="increment" />
		</id>
		<property name="Uuid"/>
		<property name="Criacao" not-null="true"/>
		<property name="Apresentacao" />
		<property name="Expiracao" />
		<property name="CriadoEm" />
		<property name="ModificadoEm" />
		<property name="DeletadoEm" />
		<property name="Deletado" />
	</class>

</hibernate-mapping>
