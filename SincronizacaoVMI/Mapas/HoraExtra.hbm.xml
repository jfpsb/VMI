<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SincronizacaoVMI"
                   namespace="SincronizacaoVMI.Model">

	<class name="HoraExtra" table="hora_extra">
		<cache usage="read-write"/>
		<id name="Id" type="Int32">
			<generator class="native" />
		</id>
		<property name="Uuid" />
		<many-to-one name="Funcionario" class="SincronizacaoVMI.Model.Funcionario" column="funcionario" lazy="false" not-null="true"/>
		<many-to-one name="TipoHoraExtra" class="SincronizacaoVMI.Model.TipoHoraExtra" column="tipo" lazy="false" not-null="true" />
		<many-to-one name="LojaTrabalho" class="SincronizacaoVMI.Model.Loja" column="loja_trabalho" lazy="false" not-null="true"/>
		<property name="Mes" />
		<property name="Ano" />
		<property name="Horas" />
		<property name="Minutos" />
		<property name="Descricao" />
		<property name="CriadoEm" />
		<property name="ModificadoEm" />
		<property name="DeletadoEm" />
		<property name="Deletado" />
	</class>

</hibernate-mapping>
