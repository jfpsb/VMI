<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SincronizacaoBD"
                   namespace="SincronizacaoBD.Model">

  <class name="Loja" table="loja" dynamic-insert="true" dynamic-update="true">
    <id name="Cnpj" column="cnpj" />
    <many-to-one name="Matriz" class="SincronizacaoBD.Model.Loja" column="matriz" />
    <property name="Nome" column="nome" />
    <property name="Telefone" column="telefone" />
    <property name="Endereco" column="endereco" />
    <property name="InscricaoEstadual" column="inscricaoestadual" />
    <property name="LastUpdate" column="lastUpdate" />
  </class>

</hibernate-mapping>
