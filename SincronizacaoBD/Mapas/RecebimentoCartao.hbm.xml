<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SincronizacaoBD"
                   namespace="SincronizacaoBD.Model">

  <class name="RecebimentoCartao" table="recebimentocartao" dynamic-insert="true" dynamic-update="true">
    <composite-id>
      <key-property name="Mes" column="mes" />
      <key-property name="Ano" column="ano" />
      <key-many-to-one name="Loja" column="loja" class="SincronizacaoBD.Model.Loja" />
      <key-many-to-one name="OperadoraCartao" column="operadoracartao" class="SincronizacaoBD.Model.OperadoraCartao" />
    </composite-id>
    <property name="Recebido" column="recebido" />
    <property name="ValorOperadora" column="valoroperadora" />
    <property name="Observacao" column="observacao" />
  </class>

</hibernate-mapping>
