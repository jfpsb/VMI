<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SincronizacaoBD"
                   namespace="SincronizacaoBD.Model">

  <class name="Fornecedor" table="fornecedor" dynamic-insert="true" dynamic-update="true">
    <id name="Cnpj" column="cnpj"></id>
    <property name="Nome" column="nome" />
    <property name="Fantasia" column="fantasia" />
    <property name="Email" column="email" />
    <property name="Telefone" column="telefone" />
    <property name="LastUpdate" column="lastUpdate" />
  </class>

</hibernate-mapping>
